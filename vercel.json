{
    "version": 2,
    "builds": [
      {
        "src": "myapp/wsgi.py",
        "use": "@vercel/python"
      },
      {
        "src": "staticfiles/**/*",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app_cost_calculator/wsgi.py"
      }
    ]
  }
  