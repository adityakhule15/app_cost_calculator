{
    "version": 2,
    "builds": [
      {
        "src": "app_cost_calculator/wsgi.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app_cost_calculator/wsgi.py"
      }
    ],
    "functions": {
      "api/**/*.py": {
        "maxDuration": 10
      }
    },
    "env": {
      "PYTHON_VERSION": "3.9"
    },
    "github": {
      "silent": true
    }
  }
  